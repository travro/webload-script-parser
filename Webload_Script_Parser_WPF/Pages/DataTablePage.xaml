<Page x:Class="Webload_Script_Parser_WPF.Pages.DataTablePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Webload_Script_Parser_WPF.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="DataTablePage">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <DataGrid  Grid.Row="0" x:Name="Data_Grid_Transactions"/>
        <DataGrid  Grid.Row="1" x:Name="Data_Grid_Requests"/>
        <DataGrid  Grid.Row="2" x:Name="Data_Grid_Correlations"/>
    </Grid>



</Page>
