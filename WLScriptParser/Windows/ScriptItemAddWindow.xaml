<Window x:Class="WLScriptParser.Windows.ScriptItemAddWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WLScriptParser.Windows"
        mc:Ignorable="d"
        Title="ScriptItemAddWindow" Height="107.725" Width="732.767">
    <Grid Margin="10,0,10,0" Height="30">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="2.5*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <TextBox Grid.Column="0" x:Name="Text_Box" MaxLength="25" />
        <Button Grid.Column="1" x:Name="Add_Button" Content="Add" Click="Add_Button_Click" Margin="10,0,0,0"/>
        <Button Grid.Column="2" x:Name="Cancel_Button" Content="Cancel" Click="Cancel_Button_Click" Margin="10,0,0,0"/>
    </Grid>
</Window>
