      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WLScriptParser.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="DataTablePage">
    <Grid x:Name="Main_Grid" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Background="Gray">
        <ScrollViewer VerticalScrollBarVisibility="Auto">
            <StackPanel x:Name="Stack_Panel" Orientation="Vertical" Margin="8,10,8,10"/>           
        </ScrollViewer>
    </Grid>
</Page>
